<Window x:Class="TarifsPresse_Codipress.Fenetres.Window_Create"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Création de ..." Height="183" Width="578" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="Button_creer" Content="Créer" HorizontalAlignment="Right" Margin="0,111,10,0" Width="100" Height="22" VerticalAlignment="Top" Click="Button_Click"/>
        <Label x:Name="text_libelle" Content="Libellé du ..." HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.053,1.192" Width="272"/>
        <Label x:Name="text_identifier" Content="Identifiant ...." HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="272"/>
        <TextBox x:Name="TextBox_Libelle" Text="{Binding Path=Libelle, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Height="23" Margin="10,35,10,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="TextBox_Libelle_TextChanged"/>
        <TextBox x:Name="TextBox_Identifier" Text="{Binding Path=Identifier, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" Height="23" Margin="10,111,115,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="TextBox_Identifier_TextChanged"/>
    </Grid>
</Window>
